{
    "version": "2.0",
    "columns": 4,
    "zoom": 0.5,
    "tiles": [
    {% for value in values %}
        {
            "type": "HTTP-RAW",
            "label": "{{ value.name }}:{{ env }}",
            "params": {
                "url": "{{ value.envs[env] }}",
                "regex": "(\\d+.\\d+.\\d+)"
            }
        }{% if not loop.last %},{% endif %}
    {% endfor %}
    ]
}
